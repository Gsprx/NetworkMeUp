<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NetworkMeUp"
        tools:targetApi="31">
        <activity
            android:name=".view.LoginEmployer.LoginEmployerActivity"
            android:exported="false" />
        <activity
            android:name=".view.LoginEmployee.LoginEmployeeActivity"
            android:exported="false" />
        <activity
            android:name=".view.SignUpEmployer.SignUpEmployerActivity"
            android:exported="false" />
        <activity
            android:name=".view.SignUpEmployee.SignUpEmployeeActivity"
            android:exported="false" />
        <activity
            android:name=".view.SignUp.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".view.Login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".domain.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>